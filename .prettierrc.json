{
  "$schema": "https://json.schemastore.org/prettierrc",
  "printWidth": 120,
  "trailingComma": "all",
  "tabWidth": 4,
  "useTabs": false,
  "semi": false,
  "singleQuote": true,
  "proseWrap": "always",
  "overrides": [
    {
      "files": "*.json",
      "options": {
        "tabWidth": 2
      }
    }
  ]
}
